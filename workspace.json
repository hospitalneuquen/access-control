    "cli": {
        "defaultCollection": "@nrwl/nest"
    },
    "schematics": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "eslint"
            }
        },
        "@nrwl/react": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
